<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">



    <application
        android:name=".requestEngine.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        tools:replace="android:label"
        android:label="Рукопожатия ВК"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".requestEngine.outputActivity.OutputForPWOS" />
        <activity android:name=".settings.extendedSettings.ExtendedSettingsActivity" />
        <activity android:name=".requestEngine.outputActivity.UserDescriptionActivity" />
        <activity
            android:name=".requestEngine.outputActivity.HandShakeOutputScrollingActivity"
            android:label="@string/title_activity_hand_shake_output_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".requestEngine.InputUserInfoActivity" />
        <activity android:name=".settings.SettingsActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".MainActivity"/>
        <activity
            android:name="com.vk.sdk.VKServiceActivity"
            android:label="ServiceActivity"
            android:theme="@style/VK.Transparent" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>