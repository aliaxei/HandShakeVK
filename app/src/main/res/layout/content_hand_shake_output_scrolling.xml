<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
android:id="@+id/background"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".requestEngine.outputActivity.HandShakeOutputScrollingActivity"
    tools:showIn="@layout/activity_hand_shake_output_scrolling">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/backgroundHouses"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@drawable/supermap400"
            />




            <RelativeLayout
                android:layout_width="200dp"
                android:layout_height="100dp"
                android:layout_marginTop="60dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/firstUserImageView"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_centerInParent="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="10dp"
                    android:visibility="invisible"
                    android:background="@drawable/picture_border"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginTop="140dp"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <RelativeLayout
                    android:layout_width="130dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/firstUserEditTextName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_alignParentRight="true"
                        android:textAlignment="center"
                        android:background="@drawable/picture_border"
                        android:textColor="@color/vk_white"
                        android:layout_marginLeft="10dp"
                        android:shadowColor="#000000"
                        android:shadowDx="-3"
                        android:shadowDy="3"
                        android:shadowRadius="0.1"
                        android:textStyle="bold"
                        android:visibility="invisible"/>
                </RelativeLayout>

            </LinearLayout>

        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_marginStart="208dp"
            android:layout_marginTop="200dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/secondUserImageView"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerInParent="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/picture_border"
                android:layout_alignParentRight="true"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginStart="190dp"
            android:layout_marginTop="280dp"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="200dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/secondUserEditTextName"
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:textAlignment="center"
                    android:layout_marginRight="40dp"
                    android:background="@drawable/picture_border"
                    android:text="Name Surname"
                    android:textColor="@color/vk_white"
                    android:shadowColor="#000000"
                    android:shadowDx="-3"
                    android:shadowDy="3"
                    android:shadowRadius="0.1"
                    android:textStyle="bold"
                    android:visibility="invisible"/>
            </RelativeLayout>
        </LinearLayout>


        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_marginTop="300dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/thirdUserImageView"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                android:visibility="invisible"
                android:background="@drawable/picture_border"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="130dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="380dp"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="130dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/thirdUserEditTextName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="10dp"
                    android:textAlignment="center"
                    android:background="@drawable/picture_border"
                    android:text="Name Surname"
                    android:textColor="@color/vk_white"
                    android:shadowColor="#000000"
                    android:shadowDx="-3"
                    android:shadowDy="3"
                    android:shadowRadius="0.1"
                    android:textStyle="bold"
                    android:visibility="invisible"/>
            </RelativeLayout>

        </LinearLayout>


        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_marginStart="208dp"
            android:layout_marginTop="440dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/fouthUserImageView"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="invisible"
                android:background="@drawable/picture_border"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginStart="190dp"
            android:layout_marginTop="520dp"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="200dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/fouthUserEditTextName"
                    android:layout_width="130dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="40dp"
                    android:textAlignment="center"
                    android:onClick="onClick"
                    android:background="@drawable/picture_border"
                    android:text="Name Surname"
                    android:textColor="@color/vk_white"
                    android:shadowColor="#000000"
                    android:shadowDx="-3"
                    android:shadowDy="3"
                    android:shadowRadius="0.1"
                    android:textStyle="bold"
                    android:visibility="invisible"/>
            </RelativeLayout>

        </LinearLayout>


        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="100dp"

            android:layout_marginTop="560dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/fiveUserImageView"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="20dp"
                android:background="@drawable/picture_border"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:visibility="invisible"/>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="130dp"
            android:layout_height="30dp"
            android:layout_marginTop="640dp"
            android:layout_marginStart="10dp"
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RelativeLayout
                android:layout_width="130dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/fiveUserEditTextName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="10dp"
                    android:textAlignment="center"
                    android:background="@drawable/picture_border"
                    android:text="Name Surname"
                    android:textColor="@color/vk_white"
                    android:shadowColor="#000000"
                    android:shadowDx="-3"
                    android:shadowDy="3"
                    android:shadowRadius="0.1"
                    android:textStyle="bold"
                    android:visibility="invisible"/>
            </RelativeLayout>

        </LinearLayout>
    </RelativeLayout>
    </LinearLayout>
</ScrollView>